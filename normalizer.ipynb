   "source": [
    "name='shapes'\n",
    "\n",
    "file = open(name+'.txt', 'r')\n",
    "txt = file.read().split('\\n')\n",
    "file.close()\n",
    "\n",
    "ok=[]\n",
    "for c in txt:\n",
    "    if not c in ok:\n",
    "        ok.append(c)\n",
    "\n",
    "txt='\\n'.join(ok)\n",
    "\n",
    "\n",
    "file = open(name+'.txt', 'w')\n",
    "file.write(txt.lower())\n",
    "file.close()"
   ]
  }
